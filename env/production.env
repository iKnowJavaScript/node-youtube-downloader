TYPEORM_SYNCHRONIZE=true
TYPEORM_LOGGING=advanced-console
TYPEORM_MIGRATIONS=src/migration/*{.ts,.js}
TYPEORM_MIGRATIONS_RUN=src/migration


#
# REDIS
#
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_AUTH_EXPIRATION=86400
