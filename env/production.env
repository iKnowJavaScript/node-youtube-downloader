# Environment
NODE_ENV=production

# Server
APP_PORT=3500
APP_HOST=127.0.0.1
APP_NAME=youtube-downloader

#
# PostgreSQL DATABASE
#
DATABASE_CONNECTION=postgres
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=kingkonsole
DATABASE_NAME=downloader
